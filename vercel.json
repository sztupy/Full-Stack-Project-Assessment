{
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "outputDirectory": "api/static",
    "regions": ["fra1"],
    "build": {
        "env": {
            "NODEJS_AWS_HANDLER_NAME": "handler"
        }
    },
    "rewrites": [{ "source": "/api/(.*)", "destination": "/server/functions/app" }]
}
